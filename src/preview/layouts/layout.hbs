<html lang="de" class="no-js">                         <!--<![endif]-->
<head>
  <meta charset="utf-8">
  <meta http-equiv="X-UA-Compatible" content="IE=edge">
  <title>{{#if title}}{{title}} â€“ {{/if}}{{meta.project}}</title>
  <meta name="viewport" content="width=device-width, initial-scale=1">
  <meta name="format-detection" content="telephone=no">


  <link rel="stylesheet" href="/assets/css/main{{#unless env.dev}}.min{{/unless}}.css">
  <link rel="stylesheet" href="/preview/assets/css/main{{#unless env.dev}}.min{{/unless}}.css">

  <script src="/assets/js/head{{#unless env.dev}}.min{{/unless}}.js"></script>
  <script src="/assets/js/main{{#unless env.dev}}.min{{/unless}}.js" defer></script>

</head>
<body {{#if props.svgSprites}} data-svgsprites-options='{{props.svgSprites}}'{{/if}}
                               data-fonts='/assets/css/fonts{{#unless env.dev}}.min{{/unless}}.css'
                               data-script-main='/assets/js/main{{#unless env.dev}}.min{{/unless}}.js'>
<div class="sg_wrapper{{#if additionalLayoutClass}} {{additionalLayoutClass}}{{/if}}">
  <div class="sg_index_header">
  <svg viewBox="0 0 100 100" class="sg_logo">
    <use xlink:href="#ktzh"></use>
  </svg>
  <h1 class="sg_title sg_title--top">
    <a href="/" class="sg_logo">{{meta.project}}</a>{{#if meta.title}} / {{meta.title}}{{/if}}
  </h1>
  </div>
  <div class="sg_revisioninfo sg_revisioninfo--overview">
    <h4>Quellcodeverwaltung</h4>
    <ul>
      <li>Revisions Nummer: <a href="{{meta.gitRemoteLink}}">{{meta.gitShort}}</a></li>
      <li>Datum: {{meta.gitDate}}</li>
      <li>Branch: {{meta.gitBranch}}</li>
      <li>Build Datum: {{meta.buildDate}}</li>
    </ul>
  </div>
  {{#block "content"}}
  {{/block}}
</div>
</body>
</html>
