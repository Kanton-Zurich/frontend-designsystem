<body {{#if props.svgSprites}} data-svgsprites-options='{{props.svgSprites}}'{{/if}}
                               data-script-main='/assets/js/main{{#unless env.dev}}.min{{/unless}}.js'>
<body {{#if props.svgSprites}} data-svgsprites-options='{{props.svgSprites}}'{{/if}}>
<header class="lyt-wrapper" id="header">
  <div class="grid-x grid-margin-x">
    <div class="cell small-offset-2 medium-offset-2 large-offset-2 xlarge-offset-2 small-10 medium-9 large-8">
      <div class="mdl-page-header      " data-init="pageHeader" data-page-header-instance="pageheader1">
        <div class="lyt-wrapper">
          <div class="grid-x grid-margin-x">
            <div class="mdl-page-header__logo-container cell tiny-2 xsmall-2 small-2 medium-2 large-2 xlarge-2">
                  <span>
                    <span class="visuallyhidden">Logo des Kantons Zürich</span>
                    <svg viewBox="0 0 100 100" class="mdl-page-header__logo print-hidden">
                      <use xlink:href="#ktzh"></use>
                    </svg>
                    <svg viewBox="0 0 100 100" class="mdl-page-header__logo screen-hidden">
                      <use xlink:href="#ktzh"></use>
                    </svg>
                    <span>
                </span></span></div>
            <div class="cell  tiny-10 xsmall-10 small-10 medium-10 large-10 xlarge-10">
              <div class="mdl-page-header__breadcrumb">
                <nav>
                  <ol class="mdl-breadcrumb " data-init="breadcrumb" data-breadcrumb-instance="breadcrumb2">
                    <li class="mdl-breadcrumb__item mdl-breadcrumb__item--main mdl-breadcrumb__item--parent" data-index="0" data-breadcrumb="item">
                      <svg class="icon mdl-breadcrumb__arrow-left" focusable="false">
                        <use xlink:href="#arrow-left"></use>
                      </svg>
                      <a class="mdl-breadcrumb__link" href="../../">Kanton Zürich</a>
                    </li>
                    <li class="mdl-breadcrumb__item">
                      <svg class="icon" focusable="false">
                        <use xlink:href="#angle_right"></use>
                      </svg>
                      <a class="mdl-breadcrumb__link" href="../../styleguide.html">Living Styleguide</a>
                    </li>
                    <li class="mdl-breadcrumb__item">
                      <svg class="icon" focusable="false">
                        <use xlink:href="#angle_right"></use>
                      </svg>
                      <a class="mdl-breadcrumb__link" href="../../styleguide.html#lsg_atoms">Atome</a>
                    </li>
                    <li class="mdl-breadcrumb__item mdl-breadcrumb__item--active">
                      <svg class="icon" focusable="false">
                        <use xlink:href="#angle_right"></use>
                      </svg>
                      <span class="mdl-breadcrumb__link">{{meta.title}}</span>
                    </li>
                  </ol>
                </nav>
              </div>
              <h1 class="atm-heading mdl-page-header__title">{{meta.title}}</h1>
            </div>
          </div>
        </div>
      </div>
      {{#if meta.documentation}}
        <section class="sg_inner_wrapper">
          <h2 class="sg_h2 atm-heading">Dokumentation</h2>

          <div class="sg_documentation">
            {{{meta.documentation}}}
          </div>
        </section>
      {{/if}}
      <h2 class="atm-heading">Vorschau</h2>
    </div>
  </div>
</header>
	<main class="sg_wrapper sg_module {{props.defaultColorVariation}}" id="module_main">
		{{#if variants}}
			<section class="sg_inner_wrapper">
        <div class="lyt-wrapper">
          <div class="grid-x grid-margin-x">
            <div class="cell small-offset-2 medium-offset-2 large-offset-2 xlarge-offset-2 small-10 medium-9 large-8">
              <div class="cv-labels" aria-hidden="true">
                {{#each colorVariations}}
                  <input type="radio" name="colorVariations" class="color_trigger {{name}}" value="{{name}}" id="colorVariations{{@index}}"{{#unless @index}} checked{{/unless}} />
                  <label for="colorVariations{{@index}}" id="cv-label{{@index}}"></label>
                {{/each}}
              </div>
            </div>
          </div>
        </div>
				<ul class="sg_atoms">
          {{#each variants}}
              <li class="sg_atom">
                <div class="sg_atom-rendition">
                  <div class="sg_atom-label">
                    <h3 class="sg_h3">{{ meta.title }}</h3>
                    <p>{{ meta.desc }}</p>
                  </div>
                  <div class="sg_atom-demo {{#if props.isInverted}}sg_atom-demo--inverted{{/if}} {{#if meta.setLoadingClass}}sg_atom--loading{{/if}}">
                    {{#if meta.wrapInForm}}<form novalidate>{{/if}}
                    {{{meta.demo}}}
                    {{#if meta.wrapInForm}}</form>{{/if}}
                  </div>
                </div>
                <div class="sg_atom-html">
                  {{#if meta.code.html}}
                    <h4 class="sg_h4">Rendered HTML</h4>
                    <button class="sg_code__button atm-button atm-button--small atm-button--tag atm-button--tag-topic">
                      <span class="atm-button__text">Zeigen/Ausblenden</span>
                    </button>
                    <pre class="sg_code sg_code--collapsible hljs">{{{meta.code.html}}}</pre>
                  {{/if}}
                </div>
              </li>
          {{/each}}
        </ul>
			</section>
		{{else}}
			<section class="sg_inner_wrapper">
        <div class="lyt-wrapper">
          <div class="grid-x grid-margin-x">
            <div class="cell small-offset-2 medium-offset-2 large-offset-2 xlarge-offset-2 small-10 medium-9 large-8">
              <div class="cv-labels" aria-hidden="true">
                {{#each colorVariations}}
                  <input type="radio" name="colorVariations" class="color_trigger {{name}}" value="{{name}}" id="colorVariations{{@index}}"{{#unless @index}} checked{{/unless}} />
                  <label for="colorVariations{{@index}}" id="cv-label{{@index}}"></label>
                {{/each}}
              </div>
            </div>
          </div>
        </div>
				<h2 class="sg_h2">Vorschau des Atoms</h2>

				<ul class="sg_atoms">

          {{#each variants}}
              <li class="sg_atom-wrapper">
                <div class="sg_atom-rendition">
                  <div class="sg_atom-label">
                    <h3>{{ meta.title }}</h3>
                    <p>{{ meta.desc }}</p>
                  </div>
                  <div class="sg_atom-demo">
                    {{{meta.demo}}}
                  </div>
                </div>
                <div class="sg_atom-html">
                  {{#if meta.code.html}}
                    <h3 class="sg_h3">Rendered HTML</h3>
                    <pre class="sg_code hljs">{{{meta.code.html}}}</pre>
                  {{/if}}
                </div>
              </li>
          {{/each}}
        </ul>
			</section>
		{{/if}}

		{{#if meta.mocks}}
			<section class="sg_inner_wrapper">
				<h2 class="sg_h2">Data mocking</h2>

				{{#each meta.mocks}}
					{{#if description}}
						<h3 class="sg_h3">{{description}}</h3>
					{{/if}}

					<pre class="sg_code hljs">{{{data}}}</pre>
				{{/each}}
			</section>
		{{/if}}
		{{#block "scripts"}}
		{{/block}}
	</main>
<footer class="cv-blue">
  {{>"modules/contact/contact" wrappingElements.contact }}
  {{>"modules/footer/footer" wrappingElements.footer }}
</footer>
</body>
</html>
