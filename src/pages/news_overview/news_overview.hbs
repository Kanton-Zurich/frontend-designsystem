{{#extend "layouts/layout"}}
  {{#content "pageheader"}}
    {{> "modules/page_header/page_header" props.modules.pageHeader}}
  {{/content}}

  {{#content "content"}}
    {{> "modules/news_overview/news_overview" props.modules.newsOverview }}
    {{> "modules/newsletter_form/newsletter_form" props.modules.newsletterFormData }}
  {{/content}}

  {{#content "pagefooter"}}
    {{> "modules/contact/contact" props.modules.contactData}}
  {{/content}}

  {{#content "footer"}}
    {{> "modules/footer/footer" props.modules.footerData}}
    {{> "modules/back_to/back_to" props.modules.backToData}}
  {{/content}}
{{/extend}}
