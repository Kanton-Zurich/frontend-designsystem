{{#extend "layouts/layout"}}
  {{#content "banner"}}
    {{> "modules/banner/banner" props.modules.banner }}
  {{/content}}
  {{#content "header"}}
    {{> "modules/page_header/page_header" props.modules.pageHeaderData.variants.colored.props}}
  {{/content}}
  {{#content "content"}}
    {{> "modules/anchornav/anchornav" props.modules.anchorNav }}
    {{> "modules/content_nav/content_nav" props.modules.contentNavData }}
    {{> "modules/service_list/service_list" props.modules.serviceListData }}
    {{> "modules/accordion/accordion" props.modules.accordionData }}
    {{> "modules/contact/contact" props.modules.contactData}}
    {{> "modules/news_teaser/news_teaser" props.modules.newsTeaserData }}
    {{> "modules/related_content/related_content" props.modules.releatedContentData}}
    {{> "modules/tag_group/tag_group" props.modules.tagGroupData}}
  {{/content}}
  {{#content "footer"}}
    {{> "modules/footer/footer" props.modules.footerData }}
    {{> "modules/back2top/back2top" props.modules.back2topData}}
    {{> "modules/lang_switch/lang_switch" props.modules.langSwitchData}}
  {{/content}}
{{/extend}}
