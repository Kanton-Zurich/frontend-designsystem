{{#extend "layouts/layout_detail"}}
  {{#content "pageheader"}}
    {{> "modules/page_header/page_header" props.modules.pageHeaderData }}
  {{/content}}

  {{#content "content"}}
    {{>"modules/metablock/metablock" props.modules.metablockData}}
    {{>"atoms/smallcaption/smallcaption" props.modules.smallcaptionData }}
    {{>"modules/download_list/download_list" props.modules.downloadListData }}
    {{> "modules/contact/contact" props.modules.contactData}}
  {{/content}}

  {{#content "pagefooter"}}
    {{> "modules/related_content/related_content" props.modules.releatedContentData}}
    {{> "modules/tag_group/tag_group" props.modules.tagGroupData}}
  {{/content}}

  {{#content "footer"}}
    {{> "modules/footer/footer" props.modules.footerData }}
    {{> "modules/back_to/back_to" props.modules.backToData}}
  {{/content}}
{{/extend}}
