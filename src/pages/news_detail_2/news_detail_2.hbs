{{#extend "layouts/layout"}}
  {{#content "header"}}
    {{> "modules/page_header/page_header" props.modules.pageHeaderData}}
  {{/content}}

  {{#content "content"}}
    {{> "modules/image_figure/image_figure" props.modules.imageFigureData.variants.default.props}}
    {{> "modules/linklist/linklist" props.modules.linklistData }}
    {{> "modules/person_card/person_card" props.modules.personCardData}}
    {{> "modules/image_gallery/image_gallery" props.modules.imageGalleryData }}
    {{> "modules/download_list/download_list" props.modules.downloadListData }}
    {{> "modules/news_teaser/news_teaser" props.modules.newsTeaserData }}
    {{> "modules/tag_group/tag_group" props.modules.tagGroupData }}
  {{/content}}

  {{#content "footer"}}
    {{> "modules/footer/footer" props.modules.footerData}}
    {{> "modules/back2top/back2top" props.modules.back2topData}}
  {{/content}}
{{/extend}}
