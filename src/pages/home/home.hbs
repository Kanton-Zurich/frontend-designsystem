{{#extend "layouts/layout"}}
  {{#content "header"}}
    <header>
      {{> "modules/page_header/page_header" props.modules.pageHeaderData}}
    </header>
  {{/content}}

	{{#content "content"}}
    <h1>(--THEMELISTE FILTER PLACEHOLDER--)</h1>
    {{> "modules/content_nav/content_nav" props.modules.contentNavData}}
    {{> "modules/news_teaser/news_teaser" props.modules.newsTeaserData }}
	{{/content}}

  {{#content "footer"}}
    <footer id="contentinfo">
      {{> "modules/contact/contact" props.modules.contactData}}
      <h1>(--FOOTER PLACEHOLDER--)</h1>
      <h1 class="visuallyhidden">Fussbereich</h1>
    </footer>
  {{/content}}
{{/extend}}
