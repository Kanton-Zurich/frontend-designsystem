{{#extend "layouts/layout"}}
  {{#content "pageheader"}}
    {{> "modules/page_header/page_header" props.modules.pageHeaderData}}
  {{/content}}

	{{#content "content"}}
    {{> "modules/topiclist/topiclist" props.modules.topiclist}}
    {{> "modules/image_gallery/image_gallery" props.modules.imageGallery }}
    {{> "modules/locations/locations" props.modules.locations }}
	{{/content}}

  {{#content "pagefooter"}}
    {{> "modules/contact/contact" props.modules.contact}}
  {{/content}}

  {{#content "footer"}}
    {{> "modules/footer/footer" props.modules.footerData}}
    {{> "modules/back_to/back_to" props.modules.backToData}}
  {{/content}}
{{/extend}}
