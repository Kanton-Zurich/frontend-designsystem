{{#extend "layouts/layout_detail"}}
  {{#content "pageheader"}}
    {{> "modules/page_header/page_header" props.modules.pageHeader}}
  {{/content}}

  {{#content "content"}}
    <div class="mdl-anchornav__wrapper">
      {{> "modules/lead_section/lead_section" props.modules.leadSection}}
      {{> "modules/comment_introduction/comment_introduction" props.modules.commentIntroduction}}
      {{> "modules/richtext/richtext" props.modules.richtext}}
    </div>

    {{> "modules/table/table" props.modules.table}}
    {{> "modules/image_figure/image_figure" props.modules.imageFigure}}
    {{> "modules/image_gallery/image_gallery" props.modules.imageGallery}}
    {{> "modules/video/video" props.modules.video}}
    {{> "modules/download_list/download_list" props.modules.downloadList}}
    {{> "modules/linklist/linklist" props.modules.linkList}}
    {{> "modules/accordion/accordion" props.modules.accordion}}
    {{> "modules/locations/locations" props.modules.locations}}
    {{> "modules/service_list/service_list" props.modules.serviceList}}
    {{> "modules/linklist/linklist" props.modules.moreInfo}}
    {{> "modules/linklist/linklist" props.modules.lawsList}}
    {{> "modules/linklist/linklist" props.modules.decisionList}}
    {{> "modules/quote/quote" props.modules.quote}}
    {{> "modules/texthighlight/texthighlight" props.modules.texthighlight}}
    {{> "modules/newsletter_form/newsletter_form" props.modules.newsletterForm}}

    {{> "modules/teaser/teaser" props.modules.teaser}}
    {{> "modules/publication_teaser/publication_teaser" props.modules.publicationTeaser}}

    <h2 class="atm-heading">Details</h2>
    <h3 class="atm-heading">Weiterführende Informationen</h3>
    {{> "modules/linklist/linklist" props.modules.detailsMoreInfo}}
    <h3 class="atm-heading">Zugehöriges Legislaturziel</h3>
    {{> "modules/objectives_list/objectives_list" props.modules.objectivesList}}
    <h3 class="atm-heading">Politikbereich</h3>
    {{> "modules/linklist/linklist" props.modules.policyArea}}
  {{/content}}

  {{#content "pagefooter"}}
    {{> "modules/tag_group/tag_group" props.modules.tagGroup}}
    {{> "modules/contact/contact" props.modules.contact}}
  {{/content}}

  {{#content "footer"}}
    {{> "modules/footer/footer" props.modules.footer}}
  {{/content}}
{{/extend}}
