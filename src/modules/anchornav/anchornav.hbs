<div class="mdl-anchornav" data-init="anchornav" id="toc">
  <div class="lyt-wrapper">
    <div class="grid-x grid-margin-x">
      <div class="cell tiny-12 xsmall-12 small-10 medium-10 large-10 xlarge-10 small-offset-2 medium-offset-2 large-offset-2 xlarge-offset-2">

        <div class="mdl-anchornav__content">
          <h{{{anchornavTitle.level}}} class="atm-heading">{{ anchornavTitle.title }}</h{{{anchornavTitle.level}}}>
          <div class="mdl-anchornav__ctrl mdl-anchornav__ctrl--left">
            <button>
              <span class="visuallyhidden">Nach links scrollen</span>
              <svg class="icon atm-button__icon">
                <use xlink:href="#angle_left"></use>
              </svg>
            </button>
          </div>

          <div class="mdl-anchornav__list-wrapper" data-anchornav="scroll-area">
            <ul class="mdl-anchornav__list">
              {{#each anchornavItems}}
                <li class="mdl-anchornav__item">
                  {{> "atoms/anchorlink/anchorlink" this}}
                </li>
              {{/each}}
            </ul>
          </div>

          <div class="mdl-anchornav__ctrl mdl-anchornav__ctrl--right">
            <button>
              <span class="visuallyhidden">Nach rechts scrollen</span>
              <svg class="icon atm-button__icon">
                <use xlink:href="#angle_right"></use>
              </svg>
            </button>
          </div>
        </div>

      </div>
    </div>
  </div>
</div>

