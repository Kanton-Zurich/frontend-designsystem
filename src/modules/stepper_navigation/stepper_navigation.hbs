<ol class="mdl-stepper_navigation" data-init="stepperNavigation">
  {{#each steps}}
	  <li>
      <button class="mdl-stepper_navigation__step" data-stepper_navigation="step" data-step="{{@index}}" disabled>
        <svg class="icon mdl-stepper_navigation__step-checkmark" focusable="false">
          <use xlink:href="#check"></use>
        </svg>
        <svg class="icon mdl-stepper_navigation__step-dots" focusable="false">
          <use xlink:href="#overflow"></use>
        </svg>
        <span class="mdl-stepper_navigation__step-ordinal" data-stepper_navigation="number"></span>
        <span class="mdl-stepper_navigation__step-title">{{this}}</span>
      </button>
    </li>
  {{/each}}
  {{#ifLongerThan steps 5}}
    <li>
      <button class="mdl-stepper_navigation__step mdl-stepper_navigation__step--multiple" data-stepper_navigation="multipleBefore">
        <svg class="icon mdl-stepper_navigation__step-dots" focusable="false">
            <use xlink:href="#overflow"></use>
          </svg>
      </button>
      {{#if ../contextMenuBefore}}{{> "modules/context_menu/context_menu" ../contextMenuBefore}}{{/if}}
    </li>
    <li>
      <button class="mdl-stepper_navigation__step mdl-stepper_navigation__step--multiple" data-stepper_navigation="multipleAfter">
        <svg class="icon mdl-stepper_navigation__step-dots" focusable="false">
            <use xlink:href="#overflow"></use>
          </svg>
      </button>
      {{#if ../contextMenuAfter}}{{> "modules/context_menu/context_menu" ../contextMenuAfter}}{{/if}}
    </li>
  {{/ifLongerThan}}
</ol>
