<figure{{#if anchorReference}} id="{{anchorReference}}"{{/if}} class="mdl-video {{#if isFullwidth}}mdl-video--fullwidth{{/if}} {{#if isHeader}}mdl-video--header{{/if}}" data-init="video">
  {{#if title}}<h{{headingLevel}} class="atm-heading">{{video_title}}</h{{headingLevel}}>{{/if}}
  <div class="mdl-video__wrapper">
  <iframe
    class="mdl-video__frame"
    src="https://www.youtube.com/embed/{{videoId}}?modestbranding=1&rel=0&showInfo=0"
    height="768"
    width="432"
    allowfullscreen
    aria-describedby="mdl-video__{{videoId}}__alt-text"
  ></iframe>
  </div>
  <figcaption class="mdl-video__caption">
    {{> "atoms/figcaption/figcaption" caption}}
    <span id="mdl-video__{{videoId}}__alt-text" class="visuallyhidden">{{alt}}</span>
  </figcaption>
</figure>
