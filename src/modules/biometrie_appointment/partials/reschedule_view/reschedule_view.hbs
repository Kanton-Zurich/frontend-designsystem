{{#with messages}}
  <div class="mdl-biometrie_appointment__reschedule">

    <div class="mdl-biometrie_appointment__reschedule__allopen">
      <p class="atm-lead">{{ otherSlotsHeading }}</p>

      <div class="mdl-biometrie_appointment__reschedule__allopen__wrapper">
          <div class="group-wrapper">
            <ul class="mdl-button_group" data-init="buttonGroup">
                <li class="mdl-button_group__item">
                    {{> "atoms/button/button" prevWeekBtn}}
                </li>
                <li class="mdl-button_group__item details hidden-small-up">
                    {{#if withMockedData}}
                        <div class="strong">08.07.2019-12.07.2019</div>
                        <div class="strong lighter">KW 29</div>
                    {{else}}
                        <div class="strong" data-biometrie_appointment="selectedWeekRange"></div>
                        <div class="strong lighter" data-biometrie_appointment="selectedWeek"></div>
                    {{/if}}
                </li>
                <li class="mdl-button_group__item">
                    {{> "atoms/button/button" nextWeekBtn}}
                </li>
            </ul>
            {{#if withMockedData}}
                <strong class="btn-line hidden-small-down atm-heading">08.07.2019-12.07.2019</strong>
                <strong class="btn-line lighter hidden-small-down">KW 29</strong>
            {{else}}
                <strong class="btn-line hidden-small-down atm-heading" data-biometrie_appointment="selectedWeekRange"></strong>
                <strong class="btn-line lighter hidden-small-down" data-biometrie_appointment="selectedWeek"></strong>
            {{/if}}
          </div>
        <div data-biometrie_appointment="otherSlotsSelect" class="mdl-biometrie_appointment__reschedule__allopen__slotselect" {{#if withMockedData}}data-selected-weekday="2"{{/if}}>
            {{#if withMockedData}}
                <div class="slotselect-headrow">
                    <div data-biometrie_appointment="weekDayHead" class="weekday-head">
                        <div class="weekday-head__day_short">Mo</div>
                        <div class="weekday-head__day">Montag</div>
                        <div class="weekday-head__date">08</div>
                    </div>
                    <div data-biometrie_appointment="weekDayHead" class="weekday-head">
                        <div class="weekday-head__day_short">Di</div>
                        <div class="weekday-head__day">Dienstag</div>
                        <div class="weekday-head__date">09</div>
                    </div>
                    <div data-biometrie_appointment="weekDayHead" class="weekday-head">
                        <div class="weekday-head__day_short">Mi</div>
                        <div class="weekday-head__day">Mittwoch</div>
                        <div class="weekday-head__date">10</div>
                    </div>
                    <div data-biometrie_appointment="weekDayHead" class="weekday-head">
                        <div class="weekday-head__day_short">Do</div>
                        <div class="weekday-head__day">Donnerstag</div>
                        <div class="weekday-head__date">11</div>
                    </div>
                    <div data-biometrie_appointment="weekDayHead" class="weekday-head">
                        <div class="weekday-head__day_short">Fr</div>
                        <div class="weekday-head__day">Freitag</div>
                        <div class="weekday-head__date">12</div>
                    </div>
                </div>
                <div class="slotselect-slotsrow">

                    <div data-biometrie_appointment="weekDayColumn" class="weekday-column column">
                        <div data-biometrie_appointment="weekDaySlots" class="weekday-slotscontainer">
                            <div class="weekday-slot">
                                <button class="atm-button atm-button--secondary atm-button--small" data-biometrie_appointment="timeSlotSelect">
                                    <span class="atm-button__text">
                                        <span>08:00-08:15</span>
                                    </span>
                                </button>
                            </div>
                            <div class="weekday-slot">
                                <button class="atm-button atm-button--secondary atm-button--small" data-biometrie_appointment="timeSlotSelect">
                                    <span class="atm-button__text ">
                                        <span>11:15-11:30</span>
                                    </span>
                                </button>
                            </div>
                            <div class="weekday-slot">
                                <button class="atm-button atm-button--secondary atm-button--small" data-biometrie_appointment="timeSlotSelect">
                                    <span class="atm-button__text ">
                                        <span>12:30-12:45</span>
                                    </span>
                                </button>
                            </div>
                            <div class="weekday-slot">
                                <button class="atm-button atm-button--secondary atm-button--small" data-biometrie_appointment="timeSlotSelect">
                                    <span class="atm-button__text ">
                                        <span>16:15-16:30</span>
                                    </span>
                                </button>
                            </div>
                        </div>
                    </div>
                    <div data-biometrie_appointment="weekDayColumn" class="weekday-column no-slots-available">
                        <div data-biometrie_appointment="weekDaySlots" class="weekday-slotscontainer">

                        </div>
                        <div class="no-openslots-msg">
                            {{ noOpenSlotsMsg }}
                        </div>
                    </div>
                    <div data-biometrie_appointment="weekDayColumn" class="weekday-column column">
                        <div data-biometrie_appointment="weekDaySlots" class="weekday-slotscontainer">
                            <div class="weekday-slot">
                                <button class="atm-button atm-button--secondary atm-button--small selected" data-biometrie_appointment="timeSlotSelect">
                                    <span class="atm-button__text ">
                                        <span>08:30-08:45</span>
                                    </span>
                                </button>
                            </div>
                            <div class="weekday-slot">
                                <button class="atm-button atm-button--secondary atm-button--small" data-biometrie_appointment="timeSlotSelect">
                                    <span class="atm-button__text ">
                                        <span>09:45-10:00</span>
                                    </span>
                                </button>
                            </div>
                        </div>
                    </div>
                    <div data-biometrie_appointment="weekDayColumn" class="weekday-column column">
                        <div data-biometrie_appointment="weekDaySlots" class="weekday-slotscontainer">
                            <div class="weekday-slot">
                                <button class="atm-button atm-button--secondary atm-button--small" data-biometrie_appointment="timeSlotSelect">
                                    <span class="atm-button__text ">
                                        <span>08:00-08:15</span>
                                    </span>
                                </button>
                            </div>
                            <div class="weekday-slot">
                                <button class="atm-button atm-button--secondary atm-button--small" data-biometrie_appointment="timeSlotSelect">
                                    <span class="atm-button__text ">
                                        <span>08:15-08:30</span>
                                    </span>
                                </button>
                            </div>
                            <div class="weekday-slot">
                                <button class="atm-button atm-button--secondary atm-button--small" data-biometrie_appointment="timeSlotSelect">
                                    <span class="atm-button__text ">
                                        <span>08:45-09:00</span>
                                    </span>
                                </button>
                            </div>
                            <div class="weekday-slot">
                                <button class="atm-button atm-button--secondary atm-button--small" data-biometrie_appointment="timeSlotSelect">
                                    <span class="atm-button__text ">
                                        <span>09:00-09:15</span>
                                    </span>
                                </button>
                            </div>
                            <div class="weekday-slot">
                                <button class="atm-button atm-button--secondary atm-button--small" data-biometrie_appointment="timeSlotSelect">
                                    <span class="atm-button__text ">
                                        <span>09:45-10:00</span>
                                    </span>
                                </button>
                            </div>
                                <div class="weekday-slot">
                                    <button class="atm-button atm-button--secondary atm-button--small" data-biometrie_appointment="timeSlotSelect">
                                        <span class="atm-button__text ">
                                            <span>10:15-10:30</span>
                                        </span>
                                    </button>
                                </div>
                                <div class="weekday-slot">
                                    <button class="atm-button atm-button--secondary atm-button--small" data-biometrie_appointment="timeSlotSelect">
                                        <span class="atm-button__text ">
                                            <span>10:30-10:45</span>
                                        </span>
                                    </button>
                                </div>
                                <div class="weekday-slot">
                                    <button class="atm-button atm-button--secondary atm-button--small" data-biometrie_appointment="timeSlotSelect">
                                        <span class="atm-button__text ">
                                            <span>11:15-11:30</span>
                                        </span>
                                    </button>
                                </div>
                                <div class="weekday-slot">
                                    <button class="atm-button atm-button--secondary atm-button--small" data-biometrie_appointment="timeSlotSelect">
                                        <span class="atm-button__text ">
                                            <span>11:30-11:45</span>
                                        </span>
                                    </button>
                                </div>
                                <div class="weekday-slot">
                                    <button class="atm-button atm-button--secondary atm-button--small" data-biometrie_appointment="timeSlotSelect">
                                        <span class="atm-button__text ">
                                            <span>12:00-12:15</span>
                                        </span>
                                    </button>
                                </div>
                                <div class="weekday-slot">
                                    <button class="atm-button atm-button--secondary atm-button--small" data-biometrie_appointment="timeSlotSelect">
                                        <span class="atm-button__text ">
                                            <span>13:15-13:30</span>
                                        </span>
                                    </button>
                                </div>
                                <div class="weekday-slot">
                                    <button class="atm-button atm-button--secondary atm-button--small" data-biometrie_appointment="timeSlotSelect">
                                        <span class="atm-button__text ">
                                            <span>13:45-14:00</span>
                                        </span>
                                    </button>
                                </div>
                                <div class="weekday-slot">
                                    <button class="atm-button atm-button--secondary atm-button--small" data-biometrie_appointment="timeSlotSelect">
                                        <span class="atm-button__text ">
                                            <span>14:30-14:45</span>
                                        </span>
                                    </button>
                                </div>
                                <div class="weekday-slot">
                                    <button class="atm-button atm-button--secondary atm-button--small" data-biometrie_appointment="timeSlotSelect">
                                        <span class="atm-button__text ">
                                            <span>16:15-16:30</span>
                                        </span>
                                    </button>
                                </div>
                        </div>
                    </div>
                    <div data-biometrie_appointment="weekDayColumn" class="weekday-column column">
                        <div data-biometrie_appointment="weekDaySlots" class="weekday-slotscontainer">
                            <div class="weekday-slot">
                                <button class="atm-button atm-button--secondary atm-button--small" data-biometrie_appointment="timeSlotSelect">
                                    <span class="atm-button__text ">
                                        <span>09:15-09:30</span>
                                    </span>
                                </button>
                            </div>
                        </div>
                    </div>
                </div>

            {{else}}
                <div class="slotselect-headrow">
                    {{#times 5}}
                        <div data-biometrie_appointment="weekDayHead" class="weekday-head"></div>
                    {{/times}}
                </div>
                <div class="slotselect-slotsrow">
                    {{#times 5}}
                        <div data-biometrie_appointment="weekDayColumn" class="weekday-column column">
                            <div data-biometrie_appointment="weekDaySlots" class="weekday-slotscontainer">

                            </div>
                            <div class="no-openslots-msg">
                                {{ noOpenSlotsMsg }}
                            </div>
                        </div>
                    {{/times}}
                </div>
             {{/if}}

        </div>
        <div data-biometrie_appointment="selection-details__wrapper" class="mdl-biometrie_appointment__reschedule__selectiondetails{{#if withMockedData}}  show{{/if}} {{#if isSlotFull}}none{{/if}}">
          <div class="selection-details__blocked {{#if isSlotFull}}show{{/if}}" data-biometrie_appointment="slotFullMsg" >
            {{> "modules/notification/notification" slotFullNotification }}
          </div>

          <div class="mdl-biometrie_appointment__reschedule__selectiondetails__wrapper">
                <div class="atm-heading selection-details__byselection">Gewählter Termin</div>
                <div class="atm-heading selection-details__next">Nächster freier Termin</div>
                <div class="atm-heading selection-details__none">Bitte wählen Sie einen Termin</div>
                {{#if withMockedData}}
                    <div class="atm-heading selection-details__date">Mittwoch, 03.07.2019 08:30-08:45</div>
                    <div class="atm-paragraph selection-details__counter">An diesem Termin sind noch 4 Schalter frei.</div>
                {{else}}
                    <div class="atm-heading selection-details__date" data-biometrie_appointment="selection-details__date"></div>
                    <div class="atm-paragraph selection-details__counter" data-biometrie_appointment="selection-details__capacity"></div>
                {{/if}}
          </div>
            {{> "atoms/button/button" scheduleSelectedBtn}}
        </div>
      </div>
    </div>

  </div>

  <div class="mdl-biometrie_appointment__contenttemplate" data-biometrie_appointment="capacityMsgTemplate">
      {{ slotCapacityMsg }}
  </div>

  <div class="mdl-biometrie_appointment__contenttemplate"  data-biometrie_appointment="slotSelectTemplate">
      <div class="weekday-slot">
          {{> "atoms/button/button" slotSelectBtn}}
      </div>
  </div>

  <div class="mdl-biometrie_appointment__contenttemplate"  data-biometrie_appointment="slotTimescaleTemplate">
      <div class="slot lighter">
          {timeString}
      </div>
  </div>

{{/with}}
